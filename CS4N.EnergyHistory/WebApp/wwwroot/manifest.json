{
  "sap.app": {
    "id": "CS4N.EnergyHistory",
    "title": "Energy-History",
    "type": "application",
    "applicationVersion": "0.1",
    "i18n": "i18n/i18n.properties"
  },
  "sap.ui": {
    "fullWidth": true
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.120.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "rootView": {
      "viewName": "CS4N.EnergyHistory.view.App",
      "type": "XML"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewPath": "CS4N.EnergyHistory.view",
        "controlId": "rootControl",
        "controlAggregation": "pages",
        "viewType": "XML"
      },
      "routes": [
        {
          "name": "Cockpit",
          "pattern": "",
          "target": "cockpit"
        },
        {
          "name": "SolarStationDefinitionOverview",
          "pattern": "SolarStationDefinition/Overview",
          "target": "stationDefinitionOverview"
        },
        {
          "name": "SolarStationDefinition",
          "pattern": "SolarStationDefinition/:guid:",
          "target": "definition"
        },
        {
          "name": "SolarStationData",
          "pattern": "SolarStationData/{guid}",
          "target": "data"
        },
        {
          "name": "SolarStationDataEdit",
          "pattern": "SolarStationData/{guid}/Edit",
          "target": "stationDataEdit"
        },
        {
          "name": "SolarStationDataEditYear",
          "pattern": "SolarStationData/{guid}/Edit/Year",
          "target": "stationDataEditYear"
        },
        {
          "name": "EletricMeterDefinitionOverview",
          "pattern": "EletricMeter/Overview",
          "target": "eletricMeterDefinitionOverview"
        }
      ],
      "targets": {
        "cockpit": {
          "viewName": "Cockpit",
          "viewLevel": 0
        },
        "stationDefinitionOverview": {
          "viewName": "SolarStationDefinitionOverview",
          "viewLevel": 1
        },
        "definition": {
          "viewName": "SolarStationDefinition",
          "viewLevel": 2
        },
        "data": {
          "viewName": "SolarStationData",
          "viewLevel": 2
        },
        "stationDataEdit": {
          "viewName": "SolarStationDataEdit",
          "viewLevel": 3
        },
        "stationDataEditYear": {
          "viewName": "SolarStationDataEditYear",
          "viewLevel": 4
        },
        "eletricMeterDefinitionOverview": {
          "viewName": "EletricMeterDefinitionOverview",
          "viewLevel": 1
        }
      }
    }
  }
}