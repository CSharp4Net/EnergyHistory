<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:unified="sap.ui.unified">
  <Dialog icon="sap-icon://upload" title="{i18n>title_ImportStationData}">
    <content>
      <f:Form editable="true">
        <f:layout>
          <f:ResponsiveGridLayout labelSpanL="4" labelSpanM="4" labelSpanS="4" />
        </f:layout>
        <f:formContainers>
          <f:FormContainer>
            <f:formElements>
              <f:FormElement label="{i18n>text_File}">
                <f:fields>
                  <unified:FileUploader />
                </f:fields>
              </f:FormElement>
            </f:formElements>
          </f:FormContainer>
          <!--<f:FormContainer>
            <f:formElements>
              <f:FormElement label="{i18n>text_DateFrom}">
                <f:fields>
                  <DatePicker displayFormat="{/filter/dateFormat}" 
                              value="{/filter/dateFrom}" 
                              valueFormat="yyyy-MM-dd"/>
                </f:fields>
              </f:FormElement>
              <f:FormElement label="{i18n>text_DateTo}">
                <f:fields>
                  <DatePicker displayFormat="{/filter/dateFormat}" 
                              value="{/filter/dateTo}" 
                              valueFormat="yyyy-MM-dd"/>
                </f:fields>
              </f:FormElement>
            </f:formElements>
          </f:FormContainer>-->
        </f:formContainers>
      </f:Form>
    </content>
    <footer>
      <Toolbar>
        <Button icon="sap-icon://decline" text="{i18n>text_Abort}" type="Reject" press="onImportDialogAbortPress" />
        <ToolbarSpacer/>
        <Button icon="sap-icon://accept" text="{i18n>text_Submit}" type="Accept" press="onImportDialogSubmitPress" />
      </Toolbar>
    </footer>
  </Dialog>
</core:FragmentDefinition>