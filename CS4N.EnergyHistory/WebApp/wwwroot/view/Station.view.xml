<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" controllerName="CS4N.EnergyHistory.controller.Station">
	<Page id="myPage" busyIndicatorDelay="100" titleAlignment="Center">
		<customHeader>
			<Bar>
        <contentLeft>
          <Button type="Back" press="onBackPress"/>
        </contentLeft>
				<contentMiddle>
					<Image src="image/flash-energy-64.png" height="2rem"/>
					<Title text="{/pageTitle}"/>
				</contentMiddle>
			</Bar>
		</customHeader>
		<content>
      <Panel width="auto" id="canvasElementPanel" height="100%">
        <Toolbar class="sapUiTinyMarginBottom">
          <Select>
            
          </Select>
          <ToolbarSpacer/>
          <Button icon="sap-icon://edit" press="onEditPress" type="Emphasized"/>
        </Toolbar>
        <content></content>
      </Panel>
    </content>
	</Page>
</mvc:View>